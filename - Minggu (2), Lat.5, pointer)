#include <iostream>
#include <conio.h>
#define MAX 5
using namespace std;

int main(){
	cout << " Nama        = Ikko Cahya Awinata " <<endl;
	cout << " Kelas       = Teknik Informatika A 2022 " <<endl;
	cout << " NIM         = 22051204038" <<endl;
	cout << " Mata Kuliah = Struktur Data" <<endl;
	cout << " =======================================" <<endl;
	cout<<endl;
	cout << "Latihan Nomor 5" <<endl;
	int a[MAX];
	int *pa; pa = a;//atau pa = &a[0]
	
	for (int i = 0; i < MAX; i++) {
	cout << " Masukkan Nilai " << i+1 << " : " ;
	cin >> a[i];
	}
	cout << endl;
	for (int i = 0; i < MAX; i++) {
	cout << " Nilai a[" << i << "] = " << *pa << endl;
	pa++;
	}
	getch();
}
